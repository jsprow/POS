{
  "name": "rewards_app",
  "productName": "Rewards App",
  "version": "0.1.0",
  "description": "Repleotech POS integration for loyalty programs",
  "license": "MIT",
  "repository": "jsprow/POS",
  "author": "Repleotech",
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "package": "electron-packager . --out=dist --asar --overwrite --arch=x64 --platform=win32,darwin",
    "build-js": "rollup -c -f iife connect.js -o main.js",
    "uglify-js": "uglifyjs main.js -cmo main.min.js",
    "pack": "build --dir --win",
    "dist": "build --win"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron-debug": "^1.0.0",
    "electron-updater": "^1.10.4",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^1.0.1",
    "electron-builder": "^15.6.2",
    "electron-packager": "^8.0.0",
    "xo": "^0.16.0"
  },
  "build": {
    "appId": "com.electron.rewards_app",
    "mac": {
      "category": "rewards_app.app.category.shopping"
    }
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
